        <inactive>
            <r>255</r>
            <g>255</g>
            <b>200</b>
            <a>200</a>
        </inactive>
    </colour>
    <text_box>
        <text_attributes>
            <font>default</font>
            <default_text>Load</default_text>
        </text_attributes>
        <options>
            <enable_alphabet>1</enable_alphabet>
            <enable_numerics>1</enable_numerics>
            <enable_input>1</enable_input>
            <delete_initial_text_on_input>1</delete_initial_text_on_input>
        </options>
        <text_string_size>-1</text_string_size>
        <max_characters>-1</max_characters>
        <padding>
            <x>10</x>
            <y>5</y>
        </padding>      
    </text_box>
</element>

